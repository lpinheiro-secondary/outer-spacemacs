  "tokenColors": [
    {
      "name": "Comments",
      "scope": "comment",
      "settings": {
        "foreground": "#958383"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#4ac1ce"
      }
    },
    {
      "name": "Numbers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#b46bbd"
      }
    },
    {
      "name": "Built-in constants",
      "scope": "constant.language",
      "settings": {
        "foreground": "#b46bbd"
      }
    },
    {
      "name": "User-defined constants",
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#b46bbd"
      }
    },
    {
      "name": "Variables",
      "scope": "variable",
      "settings": {
        "foreground": "#95b0fb",
        "fontStyle": ""
      }
    },
    {
      "name": "Keywords",
      "scope": "keyword",
      "settings": {
        "foreground": "#5fa7e7"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "fontStyle": "",
        "foreground": "#5fa7e7"
      }
    },
    {
      "name": "Storage types",
      "scope": "storage.type",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ee739a"
      }
    },
    {
      "name": "Class names",
      "scope": [
        "entity.name.class",
        "entity.name.type"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#5fa7e7"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "italic underline",
        "foreground": "#cc7ed5"
      }
    },
    {
      "name": "Function names",
      "scope": "entity.name.function",
      "settings": {
        "fontStyle": "",
        "foreground": "#cc7ed5"
      }
    },
    {
      "name": "Function arguments",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "",
        "foreground": "#95b0fb"
      }
    },
    {
      "name": "Tag names",
      "scope": "entity.name.tag",
      "settings": {
        "fontStyle": "",
        "foreground": "#5fa7e7"
      }
    },
    {
      "name": "Tag attributes",
      "scope": "entity.other.attribute-name",
      "settings": {
        "fontStyle": "",
        "foreground": "#cc7ed5"
      }
    },
    {
      "name": "Library functions",
      "scope": "support.function",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#cc7ed5"
      }
    },
    {
      "name": "Library constants",
      "scope": "support.constant",
      "settings": {
        "fontStyle": "",
        "foreground": "#b46bbd"
      }
    },
    {
      "name": "Library classes/types",
      "scope": [
        "support.type",
        "support.class"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ee739a"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#e0211d"
      }
    },
    {
      "name": "diff: header",
      "scope": [
        "meta.diff",
        "meta.diff.header"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#E0EDDD"
      }
    },
    {
      "name": "diff: deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#dc322f"
      }
    },
    {
      "name": "diff: changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#cb4b16"
      }
    },
    {
      "name": "diff: inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#219186"
      }
    },
		{
			"scope": [
				 "markup.heading.markdown"
			],
			"settings": {
				 "foreground": "#3ab1be",
				 "fontStyle": "bold"
			}
	 	},
		{
			"scope": [
				"markup.bold",
			],
			"settings": {
				"foreground": "#b46bbd",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"markup.italic"
			],
			"settings": {
				"foreground": "#ee739a",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"markup.quote"
			],
			"settings": {
				"foreground": "#958383"
			}
		},
		{
			"scope": [
				"markup.inline.raw"
			],
			"settings": {
				"foreground": "#4ac1ce"
			}
		}
  ]
}